{
  "name": "opentribes/core",
  "autoload": {
    "psr-4":{
      "OpenTribes\\Core\\": "src/"
    }
  },
"autoload-dev": {
  "psr-4": {
    "OpenTribes\\Core\\Tests\\": "tests/"
  }
},
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "nunomaduro/phpinsights": "^2.1",
    "vimeo/psalm": "^4.18"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "composer/package-versions-deprecated": true
    }
  }
}
